Pi = 3.14159265359

laser "findroute startx=0.5 starty=0.5 endx=4.5 endy=4.5"
wait 1

laser "localize"
laser "push t='1.0' cmd='localize'"
wait 3
turn 90

pn = $l4
eval pn
label "loop"
stringcat "getpoint p="pn
laser "$string"
wait 1
x=$l5
y=$l6
th=($l7/Pi)*180
eval x
eval y
eval th

ignoreobstacles
drivew x y th:($targetdist<0.15)
wait 1

pn=pn-1
if (pn>-1) "loop"
